// Converts a BinMap to a List of stored values.
// - A: Value type.
// - m: Input BinMap.
// = List of values stored in the BinMap.
BinMap/values : âˆ€(A: *) (BinMap A) -> (List A)
| A m = (List/map (Pair Bits A) A (Pair/get-snd Bits A) (BinMap/to-list A m))

#test:
  let map = (BinMap/empty U32)
  (BinMap/values U32 map) == []

#test: 
  let map = (BinMap/empty U32)
  let map = (BinMap/set U32 map #O{#O{#E}} 0)
  let map = (BinMap/set U32 map #I{#O{#E}} 1)
  let map = (BinMap/set U32 map #O{#I{#E}} 2)
  let map = (BinMap/set U32 map #I{#I{#E}} 3)
  (BinMap/values U32 map) == [0 1 2 3]
