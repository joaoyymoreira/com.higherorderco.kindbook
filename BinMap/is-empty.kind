// Checks if a BinMap is empty.
// - A: Value type.
// - map: The BinMap to check.
// = True if the map is empty, False otherwise.
BinMap/is-empty : âˆ€(A: *) (BinMap A) -> Bool
| A #Leaf = #True
| A #Node{#None l r} with (BinMap/is-empty A l)
. | #True  = (BinMap/is-empty A r)
. | #False = #False
| A #Node{#Some{_} _ _} = #False

#test: (BinMap/is-empty U64 (BinMap/empty U64)) == #True
#test: (BinMap/is-empty U64 (BinMap/insert U64 #Pair{#O{#E} 1} (BinMap/empty U64))) == #False
#test: (BinMap/is-empty U64 (BinMap/insert U64 #Pair{#I{#E} 2} (BinMap/insert U64 #Pair{#O{#E} 1} (BinMap/empty U64)))) == #False