use Bits/ as BS/

// Performs bitwise NOT operation on a Bits value.
// - bits: The input Bits value.
// = A new Bits value representing the bitwise NOT of the input.
BS/not
: ∀(bits: BS/Bits)
  BS/Bits
= λ{
  #E: #E{}
  #O: λbits.tail #I{(BS/not bits.tail)}
  #I: λbits.tail #O{(BS/not bits.tail)}
}